	function chkCouponAuth(coupon_idx){
		var deferred = $.Deferred();
		$.ajax({
			type	: "post",
			url		: "/api/event/coupon_auth_chk.asp",
			data	: {"coupon_idx":coupon_idx},
			dataType	: "json",
			success : function(oResult){
				deferred.resolve(oResult);
			}
		});
		return deferred.promise();
	}
